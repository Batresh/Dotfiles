super + Return
	$TERMINAL
super + shift + q
	kill -9 `xdotool getwindowfocus getwindowpid`
super + d
	rofi -show drun
super + Escape
	rofi-power
super + w
	$BROWSER
super + r
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & $FILE"'
super + h
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & htop"'
super + g
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & gotop"'
super + n
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & newsboat"'
super + shift + M
	$TERMINAL --title=SPT -e 'zsh -c "(cat ~/.config/wpg/sequences &) & spotifyd --no-daemon >/dev/null 2>&1 & spt"'
XF86MonBrightnessDown
	xbacklight -dec 15
XF86MonBrightnessUp
	xbacklight -inc 15
shift + XF86MonBrightnessDown
	xbacklight -dec 1
XF86AudioMute
	lmc mute
XF86AudioLowerVolume
	lmc down 5
XF86AudioRaiseVolume
	lmc up 5
