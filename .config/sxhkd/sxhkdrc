super + Return
	$TERMINAL
super + d
	rofi -show drun
super + r
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & $FILE"'
super + w
	$BROWSER
super + h
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & htop"'
super + g
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & gtop"'
super + n
	$TERMINAL -e 'zsh -c "(cat ~/.config/wpg/sequences &) & newsboat"'
XF86MonBrightnessDown
	xbacklight -dec 15
XF86MonBrightnessUp
	xbacklight -inc 15
XF86AudioMute
	lmc mute
XF86AudioLowerVolume
	lmc down 5
XF86AudioRaiseVolume
	lmc up 5
