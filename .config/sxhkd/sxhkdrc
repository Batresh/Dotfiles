super + Return
	$TERMINAL
super + d
	rofi -show drun
super + Escape
	dmenupower
super + w
	$BROWSER
super + r
	$TERMINAL -t vifm -e $FILE
#super + h
#	$TERMINAL --title=htop -e 'zsh -c "(cat ~/.config/wpg/sequences &) & htop"'
#super + g
#	$TERMINAL --title=gotop -e 'zsh -c "(cat ~/.config/wpg/sequences &) & gotop"'
super + n
	$TERMINAL --title=newsboat -e 'zsh -c "(cat ~/.config/wpg/sequences &) & newsboat"'
super + shift + M
	$TERMINAL -t spt -e launch-spotify
XF86MonBrightnessDown
	xbacklight -dec 15
XF86MonBrightnessUp
	xbacklight -inc 15
shift + XF86MonBrightnessDown
	xbacklight -dec 1
XF86AudioMute
	lmc mute
XF86AudioLowerVolume
	lmc down 5
XF86AudioRaiseVolume
	lmc up 5
super + Print
	dmenurecord
super + Delete
	dmenurecord kill
